[user]
	email = wmhilton@gmail.com
	name = William Hilton
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit -10
  lgb = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit -10 --branches --remotes
  ls = ls-files
  cm = commit -m
  cb = checkout -b
  rb = branch -d
  rbs = !git branch --merged develop | sed 's/^\\s*//' | grep '^feature' && echo 'Run git rbsf to delete'
  rbsf = !git branch --merged develop | sed 's/^\\s*//' | grep '^feature' | xargs git branch -d
[push]
	default = simple
[credential]
	helper = cache --timeout=345600; remember passwords for 4 days to compensate for 2FA
[color]
	status = always
[github]
	user = wmhilton
